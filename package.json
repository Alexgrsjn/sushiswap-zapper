{
  "name": "@sushiswap/zapper",
  "version": "0.2.1",
  "license": "GPLv2",
  "description": "Contracts for SushiSwap fueled by Zapper.fi",
  "files": [
    "artifacts",
    "contracts"
  ],
  "scripts": {
    "solhint": "solhint 'contracts/**/*.sol'",
    "build": "buidler prepend-spdx-license && buidler compile",
    "start": "buidler node",
    "console": "buidler console",
    "test": "buidler test",
    "deploy:buidlerevm": "buidler deploy",
    "deploy:kovan": "buidler deploy --network kovan",
    "deploy:rinkeby": "buidler deploy --network rinkeby",
    "deploy:mainnet": "buidler deploy --network mainnet",
    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.3",
    "@nomiclabs/buidler-ethers": "^2.0.0",
    "@nomiclabs/buidler-waffle": "^2.0.0",
    "@sushiswap/sdk": "^3.0.0",
    "buidler-deploy": "^0.5.18",
    "buidler-gas-reporter": "^0.1.3",
    "buidler-spdx-license-identifier": "^1.0.3",
    "canonical-weth": "^1.4.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "replace-in-file": "^6.1.0"
  },
  "dependencies": {
    "@sushiswap/core": "^1.4.1"
  }
}
